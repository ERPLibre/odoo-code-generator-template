        <xpath expr=".">
            <div data-selector=".o_demo_website_multiple_snippet_helloworld_content" data-drop-in=".row > div" />
            <div data-selector=".o_demo_website_multiple_snippet_individual_item_content" data-drop-in=".row > div" />
