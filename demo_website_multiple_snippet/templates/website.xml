<odoo>
    <template id="demo_model_portal_unit_List_show_time_item_structure" name="Portal_Time">
        <t t-call="website.layout">
            <div id="wrap">
                <div class="oe_structure">
                    <section class="pt32 pb32" t-if="not portal_time">
                        <div class="container">
                            <div class="row s_nb_column_fixed">
                                <div class="col-lg-12 s_title pt16 pb16" style="text-align: center;">
                                    <h1 class="s_title_default">
                                        <font style="font-size: 62px;">Offre non existante</font>
                                    </h1>
                                </div>
                            </div>
                        </div>
                    </section>
                    <section
                        t-if="portal_time"
                        class="s_cover parallax s_parallax_is_fixed bg-black-50 pt96 pb96 s_parallax_no_overflow_hidden"
                        data-scroll-background-ratio="1"
                        style="background-image: none;"
                    >
                        <span
                            class="s_parallax_bg oe_img_bg oe_custom_bg"
                            style="background-image: url('/web/image/website.s_cover_default_image'); background-position: 50% 0;"
                        />
                        <div class="container">
                            <div class="row s_nb_column_fixed">
                                <div class="col-lg-12 s_title" data-name="Title">
                                    <h1
                                        class="s_title_thin"
                                        style="font-size: 62px; text-align: center;"
                                        t-field="portal_time.name"
                                        t-if="portal_time.name"
                                    />
                                    <h1
                                        class="s_title_thin"
                                        style="font-size: 62px; text-align: center;"
                                        t-if="not portal_time.name"
                                    >
                                        EMPTY
                                    </h1>
                                </div>
                                <div class="col-lg-12 s_text pt16 pb16" data-name="Text" t-if="portal_time.demo_binary">
                                    <p
                                        class="lead o_default_snippet_text"
                                        style="text-align: center;"
                                        t-field="portal_time.demo_binary"
                                    />
                                </div>
                                <div
                                    class="col-lg-12 s_text pt16 pb16"
                                    data-name="Text"
                                    t-if="portal_time.demo_binary_image"
                                >
                                    <img
                                        t-if="portal_time.demo_binary_image"
                                        t-att-src="image_data_uri(portal_time.demo_binary_image)"
                                        alt="demo_binary_image"
                                        class="img img-fluid d-block mx-auto"
                                    />
                                </div>
                                <div
                                    class="col-lg-12 s_text pt16 pb16"
                                    data-name="Text"
                                    t-if="portal_time.demo_boolean"
                                >
                                    <p
                                        class="lead o_default_snippet_text"
                                        style="text-align: center;"
                                        t-field="portal_time.demo_boolean"
                                    />
                                </div>
                                <div class="col-lg-12 s_text pt16 pb16" data-name="Text" t-if="portal_time.demo_char">
                                    <p
                                        class="lead o_default_snippet_text"
                                        style="text-align: center;"
                                        t-field="portal_time.demo_char"
                                    />
                                </div>
                                <div class="col-lg-12 s_text pt16 pb16" data-name="Text" t-if="portal_time.demo_date">
                                    <p
                                        class="lead o_default_snippet_text"
                                        style="text-align: center;"
                                        t-field="portal_time.demo_date"
                                    />
                                </div>
                                <div
                                    class="col-lg-12 s_text pt16 pb16"
                                    data-name="Text"
                                    t-if="portal_time.demo_date_time"
                                >
                                    <p
                                        class="lead o_default_snippet_text"
                                        style="text-align: center;"
                                        t-field="portal_time.demo_date_time"
                                    />
                                </div>
                                <div
                                    class="col-lg-12 s_text pt16 pb16"
                                    data-name="Text"
                                    t-if="portal_time.demo_external_link"
                                >
                                    <p class="lead o_default_snippet_text" style="text-align: center;">
                                        <a
                                            target="_blank"
                                            t-attf-href="{{portal_time.demo_external_link}}"
                                            t-field="portal_time.demo_external_link"
                                        />
                                    </p>
                                </div>
                                <div class="col-lg-12 s_text pt16 pb16" data-name="Text" t-if="portal_time.demo_float">
                                    <p
                                        class="lead o_default_snippet_text"
                                        style="text-align: center;"
