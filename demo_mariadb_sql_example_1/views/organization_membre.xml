    <record id="organization_membre_view_search" model="ir.ui.view">
        <field name="name">organization_membre_search</field>
        <field name="model">organization.membre</field>
        <field name="arch" type="xml">
            <search string="Organization membre">
                <filter name="nom_complet" string="Nom complet" domain="[('nom_complet','!=',False)]" />
                <filter name="achat_regrouper" string="Achat regroup&#233;" domain="[('achat_regrouper','=',True)]" />
                <filter name="Inactive" string="Inactive Organization membre" domain="[('active','=',False)]" />
                <filter name="adresse" string="Adresse" domain="[('adresse','!=',False)]" />
                <filter
                    name="annee_naissance"
                    string="Ann&#233;e de naissance"
                    domain="[('annee_naissance','!=',False)]"
                />
                <filter name="arrondissement" string="Arrondissement" domain="[('arrondissement','!=',False)]" />
                <filter name="bottin_courriel" string="Bottin courriel" domain="[('bottin_courriel','=',True)]" />
