<?xml version= "1.0" encoding="utf-8"?>
<odoo>

  <record model="ir.ui.view" id="demo_model_internal_view_form">
    <field name="name">demo_model_internal_form</field>
    <field name="model">demo.model.internal</field>
    <field name="arch" type="xml">
      <form string="Titre">
        <sheet>
          <group>
            <field name="banana"/>
          </group>
          <group>
            <field name="name"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record model="ir.ui.view" id="demo_model_internal_view_tree">
    <field name="name">demo_model_internal_tree</field>
    <field name="model">demo.model.internal</field>
    <field name="arch" type="xml">
      <tree>
        <field name="banana"/>
        <field name="name"/>
      </tree>
    </field>
  </record>

  <record model="ir.actions.act_window" id="demo_model_internal_demo_model_internal_action_view_action_window">
    <field name="name">demo_model_internal_action_view</field>
    <field name="res_model">demo.model.internal</field>
    <field name="view_mode">list,form</field>
  </record>

</odoo>